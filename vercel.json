{
    "rewrites": [
        {
            "source": "/:path*{/}?",
            "has": [
                {
                    "type": "host",
                    "value": "^docs[.]moralis[.]com|.*[.]vercel[.]app$"
                }
            ],
            "destination": "/api/404"
        }
    ],
    "functions": {
        "api/**/*": {
            "maxDuration": 180
        }
    },
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}
