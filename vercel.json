{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "framework": null,
    "installCommand": "npm ci --production=false",
    "devCommand": "npm run start",
    "rewrites": [
        {
            "source": "/:path*{/}?",
            "has": [
                {
                    "type": "host",
                    "value": "^docs[.]moralis[.]com|.*[.]vercel[.]app$"
                }
            ],
            "destination": "/api/404"
        }
    ],
    "functions": {
        "api/**/*": {
            "maxDuration": 180
        }
    },
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        },
        {
            "source": "/fonts/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, immutable, max-age=31536000"
                }
            ]
        },
        {
            "source": "/img/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                }
            ]
        },
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).css",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DOCUSAURUS_FASTER": "true"
    },
    "build": {
        "env": {
            "NODE_OPTIONS": "--max-old-space-size=8192",
            "NODE_ENV": "production"
        }
    }
}
