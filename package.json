{
    "name": "moralis-docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "start:vercel": "vercel dev",
        "build": "NODE_OPTIONS='--max-old-space-size=8192' DOCUSAURUS_BUILD_CACHE=true docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "generate-reference": "node configs/generate.ts",
        "generate-reference:evm": "node configs/generate.ts --api=evm-docs",
        "generate-reference:streams": "node configs/generate.ts --api=streams",
        "generate-reference:solana": "node configs/generate.ts --api=solana",
        "generate-reference:force": "node configs/generate.ts --force-replace",
        "generate-code": "node configs/code/generate.ts",
        "generate-embeddings": "npx tsx configs/scripts/generate-embeddings.ts",
        "generate-guides": "npx tsx configs/scripts/generate-guides.ts",
        "fetch-intercom-articles": "node utils/ai_bot_functions/helpers/generateArticles.ts",
        "fetch-intercom-api-articles": "npx tsx utils/ai_bot_functions/helpers/generateApiArticles.ts",
        "generate-api-schema": "npx tsx utils/ai_bot_functions/helpers/generateApiSchema.ts",
        "algolia": "node ./utils/algoliaCrawlerReindex.ts",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "format": "npm run prettier:fix && npm run lint:fix",
        "format:docs": "prettier --write \"docs/**/*.{md,mdx}\"",
        "format:code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:components": "jest tests/component-imports.test.js",
        "test:images": "jest tests/image-loading.test.js",
        "test:links": "jest tests/link-validation.test.js",
        "test:api": "jest tests/api-endpoints.test.js",
        "test:pages:build": "node scripts/test-pages.js build",
        "test:pages:runtime": "node scripts/test-pages.js runtime",
        "test:pages:all": "node scripts/test-pages.js all",
        "test:pages": "npm run test:pages:all"
    },
    "dependencies": {
        "@docusaurus/core": "^3.9.1",
        "@docusaurus/faster": "^3.9.1",
        "@docusaurus/plugin-content-blog": "^3.9.1",
        "@docusaurus/plugin-google-tag-manager": "^3.9.1",
        "@docusaurus/plugin-ideal-image": "^3.9.1",
        "@docusaurus/preset-classic": "^3.9.1",
        "@docusaurus/remark-plugin-npm2yarn": "^3.9.1",
        "@docusaurus/theme-mermaid": "^3.9.1",
        "@mdx-js/react": "^3.1.1",
        "@prisma/client": "^4.11.0",
        "@radix-ui/react-avatar": "^1.0.2",
        "@radix-ui/react-dialog": "^1.0.3",
        "@radix-ui/react-popover": "^1.0.5",
        "@radix-ui/react-scroll-area": "^1.0.3",
        "@radix-ui/react-select": "^1.2.0",
        "@radix-ui/react-toast": "^1.1.3",
        "@radix-ui/react-toggle-group": "^1.0.2",
        "@segment/snippet": "^4.15.3",
        "@supabase/supabase-js": "^2.20.0",
        "@vercel/edge": "^1.1.1",
        "@vercel/postgres": "^0.4.1",
        "ajv": "^8.17.1",
        "ajv-keywords": "^5.1.0",
        "autoprefixer": "^10.4.14",
        "class-variance-authority": "^0.5.2",
        "clsx": "^1.2.1",
        "common-tags": "^1.8.2",
        "cos-similarity": "^1.1.1",
        "dotenv": "^16.0.3",
        "eventsource-parser": "^1.0.0",
        "formik": "^2.2.9",
        "gpt3-tokenizer": "^1.1.4",
        "gray-matter": "^4.0.3",
        "lodash": "^4.17.21",
        "lucide-react": "^0.144.0",
        "markdown-to-jsx": "^7.1.3",
        "mdast-util-mdx": "^2.0.1",
        "mdast-util-to-markdown": "^1.5.0",
        "micromark-extension-mdxjs": "^1.0.0",
        "node-fetch": "^2.6.7",
        "openai": "^4.15.3",
        "path-parser": "^6.1.0",
        "postcss": "^8.4.21",
        "qs": "^6.10.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^4.4.0",
        "react-markdown": "^8.0.4",
        "react-textarea-autosize": "^8.3.3",
        "readline": "^1.3.0",
        "remark-gfm": "^3.0.1",
        "remark-oembed": "^1.2.2",
        "tailwind-merge": "^1.12.0",
        "tailwindcss": "^3.3.1",
        "tailwindcss-animate": "^1.0.5",
        "unist-builder": "^3.0.1",
        "unist-util-filter": "^4.0.1",
        "web-vitals": "^3.0.4"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.5.4",
        "@docusaurus/eslint-plugin": "^3.9.1",
        "@docusaurus/module-type-aliases": "^3.9.1",
        "@tsconfig/docusaurus": "^1.0.5",
        "@types/common-tags": "^1.8.1",
        "@types/lodash": "^4.14.192",
        "@typescript-eslint/eslint-plugin": "^5.57.0",
        "@typescript-eslint/parser": "^5.57.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^30.2.0",
        "prettier": "2.8.7",
        "prisma": "^4.11.0",
        "sharp": "^0.34.4",
        "sharp-cli": "^5.2.0",
        "typescript": "^4.9.5",
        "vercel": "^28.18.4"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "overrides": {
        "@vercel/node": {
            "ts-node": "10.9.1",
            "typescript": "^4.9.5"
        }
    }
}
