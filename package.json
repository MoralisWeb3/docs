    "react-icons": "^4.4.0",
    "react-markdown": "^8.0.4",
    "react-textarea-autosize": "^8.3.3",
    "readline": "^1.3.0",
    "remark-gfm": "^3.0.1",
    "remark-oembed": "^1.2.2",
    "sharp": "^0.32.4",
